CREATE database QUERYBOX;
USE QUERYBOX;


CREATE TABLE student (
  `SID` INT NOT NULL AUTO_INCREMENT,
  `SNAME` VARCHAR(45) NOT NULL,
  `SPASSWORD` VARCHAR(45) NOT NULL,
  `SLAB` VARCHAR(45) NOT NULL,
  `SPC_NO` VARCHAR(45) NOT NULL,
  `
SPHONE_NO` VARCHAR(45) NOT NULL,
  `SEMAIL` VARCHAR(45) NOT NULL,
  `SGENDER` VARCHAR(45) NULL,
  PRIMARY KEY (`SID`));

CREATE TABLE `admin` (
  `aid` INT NOT NULL,
  `aname` VARCHAR(45)  NOT NULL,
  `apassword` VARCHAR(45)  NOT NULL,
  PRIMARY KEY (`aid`));

CREATE TABLE `faculty` (
  `FID` INT NOT NULL,
  `FNAME` VARCHAR(45) NOT NULL,
  `FPASSWORD` VARCHAR(45) NOT NULL,
  `FLAB` VARCHAR(45) NOT NULL,
  `FPHONE_NO` VARCHAR(45) NOT NULL,
  `FEMAIL_ID` VARCHAR(45) NOT NULL,
  `FGENDER` varchar(45) NOT NULL,
  PRIMARY KEY (`FID`));
  



CREATE TABLE QUERY(
QID INT NOT NULL,
SID INT references student(SID),
FID INT references faculty(FID),
QSTATUS varchar(45) NOT NULL,
PRIMARY KEY(`QID`));

CREATE TABLE FEEDBACK(
FBID INT NOT NULL,
SID INT references student(SID),
FID INT references faculty(FID),
FBRATING INT NOT NULL,
FDESCRIPTION varchar(45)
);

